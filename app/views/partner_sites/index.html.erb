<div class="container">
  <div class="row margin-b10">
    <div class="col-sm-12 col-xs-12">
      <h4>起動回数</h4>
    </div>
  </div>

  <table class='table table-striped table-bordered site-index-table'>
    <thead>
      <tr class="active">
        <th class=""><%= t('activerecord.attributes.partner_site.site_url') %></th>
        <th class="text-center"><%= sort_link(@q, :display_number) %></th>
      </tr>
    </thead>
    <tbody>
    <% if @partner_sites.size > 0 %>
      <% @partner_sites.each do |partner_site| %>
        <tr>
          <td class=""><%= partner_site.site_url %></td>
          <td class="text-center"><%= partner_site.display_number %></td>
        </tr>
      <%end%>
    <%else%>
      <tr>
        <td colspan="2" class="text-center">サイトURLは見つかりません。</td>
      </tr>
    <%end%>
    </tbody>
  </table>
  <div class='text-center'><%= paginate @partner_sites %></div>
</div>
